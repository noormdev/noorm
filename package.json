{
    "name": "noorm",
    "version": "1.0.0",
    "type": "module",
    "description": "An Ink CLI application",
    "main": "dist/cli/index.js",
    "bin": {
        "noorm": "dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node dist/cli/index.js",
        "start:init": "node dist/cli/index.js init",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint src tests",
        "lint:fix": "eslint src tests --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist coverage",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "cli",
        "ink",
        "react",
        "terminal"
    ],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=18"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@inkjs/ui": "^2.0.0",
        "@logosdx/observer": "^2.1.1",
        "@logosdx/utils": "^5.0.0",
        "better-sqlite3": "^12.5.0",
        "commander": "^11.1.0",
        "csv-parse": "^5.6.0",
        "eta": "^3.5.0",
        "ink": "^6.5.1",
        "ink-select-input": "^6.2.0",
        "ink-spinner": "^5.0.0",
        "ink-text-input": "^6.0.0",
        "json5": "^2.2.3",
        "kysely": "^0.28.8",
        "meow": "^14.0.0",
        "node-machine-id": "^1.1.12",
        "react": "^19.2.0",
        "voca": "^1.4.1",
        "yaml": "^2.8.2",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/better-sqlite3": "^7.6.13",
        "@types/node": "^24.10.1",
        "@types/pg": "^8.15.6",
        "@types/react": "^19.2.7",
        "@types/voca": "^1.4.6",
        "@vitest/coverage-v8": "^4.0.14",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "ink-testing-library": "^4.0.0",
        "mysql2": "^3.15.3",
        "pg": "^8.16.3",
        "prettier": "^3.7.3",
        "tedious": "^19.1.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0",
        "vitest": "^4.0.14"
    }
}
